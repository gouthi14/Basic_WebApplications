<?xml version="1.0" encoding="UTF-8"?>
<beans
	xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
<bean id="employee" class="Employee">
<property name="name" value="keerthi"></property>
<property name="department" value="java developer"></property>
<property name="address" value="tirupati"></property>
</bean>
<bean id="ofc" class="Office" autowire="byName">
<property name="id" value="10000128"></property>


</bean>

</beans>